
(rules PCB ESP32 Wroom energy meter
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 971)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.4)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    +3V3 GND "Net-(C1-Pad1)" "Net-(C4-Pad1)" "Net-(C5-Pad1)" "Net-(J1-Pad2)" "Net-(J1-Pad1)" "Net-(J3-Pad2)"
    "Net-(J3-Pad1)" "Net-(J4-Pad1)" "Net-(J5-Pad2)" "Net-(J5-Pad1)" "Net-(R3-Pad2)" "Net-(R6-Pad1)" "Net-(R7-Pad1)" "Net-(R9-Pad1)"
    "Net-(SW2-Pad1)" "Net-(U1-Pad37)" "Net-(U1-Pad36)" "Net-(U1-Pad33)" "Net-(U1-Pad31)" "Net-(U1-Pad30)" "Net-(U1-Pad29)" "Net-(U1-Pad28)"
    "Net-(U1-Pad27)" "Net-(U1-Pad26)" "Net-(U1-Pad24)" "Net-(U1-Pad23)" "Net-(U1-Pad22)" "Net-(U1-Pad21)" "Net-(U1-Pad20)" "Net-(U1-Pad19)"
    "Net-(U1-Pad18)" "Net-(U1-Pad17)" "Net-(U1-Pad16)" "Net-(U1-Pad14)" "Net-(U1-Pad13)" "Net-(U1-Pad12)" "Net-(U1-Pad11)" "Net-(U1-Pad10)"
    "Net-(U1-Pad9)" "Net-(U1-Pad5)" "Net-(U1-Pad4)" "Net-(U1-Pad32)" "Net-(U3-Pad13)" "Net-(U3-Pad6)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)